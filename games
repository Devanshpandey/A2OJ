#include<iostream>

using namespace std;

int main(){
    
    int n;
    int count = 0;
    cin>>n;
    int arr[n][2];
    for(int i = 0; i < n; i++){
        for(int j = 0; j < 2; j++){
            cin>>arr[i][j];
        }
    }
    for(int i = 0; i < n; i++){
        for(int j = 0; j < 2; j++){
            if(j<1){
                for(int k = 0; k < n; k++){
                    if(i+k<n){
                        if(arr[i][j]==arr[i+k][1]){
                            count++;
                        }
                    }
                }
            }
        }
    }for(int i = 0; i < n; i++){
        for(int j = 0; j < 2; j++){
            if(j==1){
                for(int k = 0; k < n; k++){
                    if(i+k<n){
                        if(arr[i][j]==arr[i+k][0]){
                            count++;
                        }
                    }
                }
            }
        }
    }
    cout<<count<<endl;
    
    return 0;
}

